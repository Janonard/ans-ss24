{
  "target": "bmv2",
  "p4info": "build/router.p4.p4info.txt",
  "bmv2_json": "build/router.json",
  "table_entries": [
    {
      "table": "MyIngress.handle_arp",
      "match": {
        "hdr.arp.target_protocol_address": "10.0.1.10",
        "hdr.arp.operation": 1
      },
      "action_name": "MyIngress.respond_arp_request",
      "action_params": {
        "router_mac": "08:00:00:00:01:00",
        "router_ip": "10.0.1.10"
      }
    }
  ],
  "table_entries_template": [
    {
      "table": "MyIngress.YOUR_TABLE_NAME",
      "default_action": true,
      "action_name": "MyIngress.drop",
      "action_params": { }
    },
    {
      "table": "MyIngress.YOUR_TABLE_NAME",
      "match": {
        "KEY_TO_MATCH": "VALUE_FOR_KEY"
      },
      "action_name": "MyIngress.YOUR_ACTION",
      "action_params": {
        "PARAM1": "VALUE FOR PARAM1",
        "PARAM2": "VALUE FOR PARAM2"
      }
    }
  ]
}
